<Window x:Class="ProjectCambridge.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectCambridge"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Orientation="Vertical">
        <TextBox x:Name="Instructions" />
        <Button x:Name="Execute" Click="Execute_Click">Enter</Button>
        <TextBlock x:Name="Results" FontFamily="Consolas" Height="200" />
        <Button x:Name="Test" Click="Test_Click">Run Tests</Button>
    </StackPanel>
</Window>
